<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="rootPane" prefHeight="800.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Stages.Controllers.ControllerFin">
   <children>
      <Label fx:id="TexteFin" alignment="CENTER" layoutY="1.0" prefHeight="800.0" prefWidth="1280.0" stylesheets="@../css/General.css" text="&#10;Merci d'avoir participé à ce Cube escape&#10;&#10;Réalisé par:&#10;&#10;Adèle Laurens&#10;&#10;Eliès Billotta&#10;&#10;Thi-Christine Nguyen&#10;&#10;Léa Serrano&#10;" textAlignment="CENTER" textFill="WHITE" textOverrun="CENTER_WORD_ELLIPSIS">
